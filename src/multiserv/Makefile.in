# Makefile
# $Id: Makefile.in,v 1.4 2004/08/05 00:03:29 mr Exp $
#

CC=@CC@
INCLUDE=-I../../include
CFLAGS=@DEBUG@ -pipe -Wall ${INCLUDE} @CFLAGS@
EXEEXT=@EXEEXT@
OBJEXT=@OBJEXT@


SRC = \
	help.c \
	stats.c

OBJS = ${SRC:%.c=%.@OBJEXT@}

all: info comp

build: info comp

info:
	@echo ""
	@echo "*                   Building MultiServ commands                      *"
	@echo " \`------------------------------------------------------------------´"

comp: ${OBJS}

.SUFFIXES: .c .@OBJEXT@

.c.@OBJEXT@:
	${CC} ${CFLAGS} -c $< -o $@

clean:
	rm -f *.@OBJEXT@

distclean:
	@rm -f Makefile
